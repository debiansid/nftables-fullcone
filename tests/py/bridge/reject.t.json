# mark 12345 ip protocol tcp reject with tcp reset
[
    {
        "match": {
            "left": {
                "meta": { "key": "mark" }
            },
	    "op": "==",
            "right": 12345
        }
    },
    {
        "match": {
            "left": {
                "payload": {
                    "field": "protocol",
                    "protocol": "ip"
                }
            },
	    "op": "==",
            "right": "tcp"
        }
    },
    {
        "reject": {
            "type": "tcp reset"
        }
    }
]

