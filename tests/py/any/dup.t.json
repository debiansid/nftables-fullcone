# dup to "lo"
[
    {
        "dup": {
            "addr": "lo"
        }
    }
]

# dup to mark map { 0x00000001 : "lo", 0x00000002 : "lo"}
[
    {
        "dup": {
            "addr": {
                "map": {
                    "left": {
                        "meta": "mark"
                    },
                    "right": {
                        "set": [
                            [ 1, "lo" ],
                            [ 2, "lo" ]
                        ]
                    }
                }
            }
        }
    }
]

