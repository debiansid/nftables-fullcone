# ip protocol tcp redirect to :100-200
[
    {
        "match": {
            "left": {
                "payload": {
                    "field": "protocol",
                    "name": "ip"
                }
            },
            "right": 6
        }
    },
    {
        "redirect": {
            "port": {
                "range": [ 100, 200 ]
            }
        }
    }
]

