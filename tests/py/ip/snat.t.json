# iifname "eth0" tcp dport 80-90 snat to 192.168.3.2
[
    {
        "match": {
            "left": {
                "meta": "iifname"
            },
            "right": "eth0"
        }
    },
    {
        "match": {
            "left": {
                "payload": {
                    "field": "dport",
                    "name": "tcp"
                }
            },
            "right": {
                "range": [ 80, 90 ]
            }
        }
    },
    {
        "snat": {
            "addr": "192.168.3.2"
        }
    }
]

