ip test-ip4 postrouting
  [ payload load 1b @ network header + 9 => reg 1 ]
  [ cmp eq reg 1 0x00000006 ]
  [ immediate reg 1 0x00000004 ]
  [ masq proto_min reg 1 proto_max reg 0 flags 0x2 ]

