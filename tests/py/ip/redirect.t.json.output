    {
        "match": {
            "left": {
                "payload": {
                    "field": "dport",
                    "name": "udp"
                }
            },
            "right": 53
        }
    },
    {
        "redirect": {
            "flags": [
                "random",
                "fully-random",
                "persistent"
            ]
        }
    }
]

