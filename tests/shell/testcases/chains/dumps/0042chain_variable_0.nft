table netdev filter1 {
	chain Main_Ingress1 {
		type filter hook ingress device "lo" priority -500; policy accept;
	}
}
table netdev filter2 {
	chain Main_Ingress2 {
		type filter hook ingress devices = { dummy0, lo } priority -500; policy accept;
	}
}
table netdev filter3 {
	chain Main_Ingress3 {
		type filter hook ingress devices = { dummy0, lo } priority -500; policy accept;
	}
}
