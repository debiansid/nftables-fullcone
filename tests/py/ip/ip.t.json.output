# ip saddr & 0xff == 1
[
    {
        "match": {
            "left": {
                "&": [
                    {
                        "payload": {
                            "field": "saddr",
                            "protocol": "ip"
                        }
                    },
                    "0.0.0.255"
                ]
            },
            "op": "==",
            "right": "0.0.0.1"
        }
    }
]

# ip saddr & 0xffff0000 == 0xffff0000
[
    {
        "match": {
            "left": {
                "payload": {
                    "field": "saddr",
                    "protocol": "ip"
                }
            },
	    "op": "==",
            "right": {
                "prefix": {
                    "addr": "255.255.0.0",
                    "len": 16
                }
            }
        }
    }
]

